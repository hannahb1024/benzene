# Postgres container settings
POSTGRES_DB=benzene
POSTGRES_USER=benzene
POSTGRES_PASSWORD=benzene

# Host-side DB connection defaults for scripts
BENZENE_DB_HOST=localhost
BENZENE_DB_PORT=5432

# Docker compose wiring
BENZENE_CONTAINER_NAME=benzene-postgres
BENZENE_PG_PORT_BIND=127.0.0.1:5432:5432
BENZENE_CSV_DIR_HOST=../../csvs
BENZENE_CSV_DIR_CONTAINER=/csvs
